{
	"name": "@cxl/dts",
	"description": "Generate a simplified AST from typescript source code",
	"home": "https://coaxialhost.com/dts",
	"license": "GPL-3.0-only",
	"version": "0.0.1",
	"scripts": {
		"build": "node build",
		"package": "npm test && node publish",
