{
	"extends": "../tsconfig.json",
	"ignore": true,
	"compilerOptions": {
		"bundledPackageName": "@cxl/docgen-runtime",
		"outDir": "../dist/docgen",
		"outFile": "../dist/docgen/runtime.bundle.js",
		"rootDir": "..",
		"module": "amd"
	},
	"include": [
		"../rx/index.ts",
		"../css/index.ts",
		"../tsx/index.ts",
		"../dom/index.ts",
		"../component/index.ts",
		"../template/index.ts",
		"../router/index.ts",
		"../ui/core.tsx",
		"../ui/theme.tsx",
		"../ui/layout.tsx",
		"../ui-fa/index.tsx",
		"../ui/dialog.tsx",
		"../ui/navigation.tsx",
		"../ui-table/index.tsx",
		"../ui/input-base.tsx",
		"../ui/checkbox.tsx",
		"../ui-router/index.tsx",
		"../ui/select.tsx",
		"runtime.tsx"
	],
	"references": [
		{ "path": "../build/tsconfig.amd.json", "prepend": true },
		{ "path": "tsconfig.json" }
	]
}
